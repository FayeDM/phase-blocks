{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "phase-blocks/cta-block",
	"title": "CTA Block",
  	"category": "widgets",
  	"icon": "megaphone",
  	"description": "Call To Action Block",
  	"supports": {
		"html": false
	},
		"attributes": {
		"headline": {
		"type": "string",
		"default": "Your H2 Headline Here"
		},
		"subhead": {
		"type": "string",
		"default": "Content to support your headline."
		},
		"buttonText": {
		"type": "string",
		"default": "Read More"
		},
		"buttonUrl": {
		"type": "string",
		"default": "#"
		},
		"screenReaderText": {
		"type": "string",
		"default": ""
		}
	},
	"textdomain": "phase-blocks",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/editor.css",
	"style": "file:./build/style.css",
	"render": "file:./build/render.php"
}
